    <count>
      <TrashCount rdf:ID="до_100_тыс"/>
    </count>
    <types>
      <Type rdf:ID="Аккумуляторы_свинцовые_отработанные_и_брак"/>
    </types>
    <time rdf:resource="#fdt_1"/>
